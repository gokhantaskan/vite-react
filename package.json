{
  "name": "vite-react",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "babelMacros": {
    "twin": {
      "preset": "emotion",
      "config": "tailwind.config.ts"
    }
  },
  "scripts": {
    "dev": "vite --open",
    "dev:force": "run-s clear \"dev --force\"",
    "build-only": "vite build",
    "build": "run-p type-check build-only",
    "build:staging": "run-p type-check && \"build-only --mode staging\"",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .js,.cjs,.mjs,.ts,.tsx --fix",
    "format": "prettier --write . -l -u --no-error-on-unmatched-pattern",
    "lf": "run-p lint format",
    "vitest": "vitest --coverage",
    "test:unit": "run-s \"vitest run\"",
    "test:unit:watch": "run-s \"vitest --ui\"",
    "test:e2e": "env-cmd -f .env.development --no-override playwright test",
    "test:e2e:watch": "env-cmd -f .env.development --no-override playwright test --ui",
    "test:e2e:ci": "env-cmd -f .env.staging --no-override playwright test",
    "clear": "rimraf test-results playwright-report dist",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@loadable/component": "^5.16.4",
    "@mui/icons-material": "^5.16.7",
    "@mui/lab": "5.0.0-alpha.173",
    "@mui/material": "^5.16.7",
    "chroma-js": "^2.6.0",
    "clsx": "^2.1.1",
    "formik": "^2.4.6",
    "i18next": "^23.15.1",
    "jotai": "^2.10.0",
    "jotai-devtools": "^0.10.1",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^15.0.2",
    "react-router-dom": "^6.26.2",
    "react-use": "^17.5.1",
    "sort-by": "^1.2.0",
    "twin.macro": "^3.4.1",
    "uuid": "^9.0.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.25.2",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.2.1",
    "@playwright/test": "^1.47.2",
    "@svgr/rollup": "^8.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/chroma-js": "^2.4.4",
    "@types/loadable__component": "^5.13.9",
    "@types/lodash": "^4.17.9",
    "@types/node": "^18.19.53",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "autoprefixer": "^10.4.20",
    "babel-plugin-import": "^1.13.8",
    "babel-plugin-macros": "^3.1.0",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "happy-dom": "^15.7.4",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "p-min-delay": "^4.0.2",
    "postcss": "^8.4.47",
    "prettier": "2.8.7",
    "prettier-eslint": "^15.0.1",
    "rimraf": "^6.0.1",
    "sass": "^1.79.3",
    "svgo": "^3.3.2",
    "tailwindcss": "^3.4.13",
    "typescript": "^4.9.5",
    "vite": "^5.4.8",
    "vite-plugin-checker": "^0.8.0",
    "vitest": "^2.1.1"
  },
  "packageManager": "pnpm@9.10.0"
}
